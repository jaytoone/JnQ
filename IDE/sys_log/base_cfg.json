{
  "version": 1,
  "formatters": {
  "form_name": {
  "format": "%(asctime)s - [%(levelname)s] - %(module)s.%(funcName)s (%(lineno)d line) - %(message)s"
  }
},

  "handlers": {
    "console": {
    "class": "logging.StreamHandler",
    "level": "WARNING",
    "formatter": "form_name",
    "stream": "ext://sys.stdout"
    },

    "file_rot": {
      "class": "logging.handlers.RotatingFileHandler",
      "level": "DEBUG",
      "formatter": "form_name",
      "filename": "test_log_dir/test_log.log",
      "maxBytes" : 1048576,
      "backupCount": 10
    }
  },

  "root": {
    "level": "DEBUG",
    "handlers": ["console", "file_rot"]
  }
}

