 #     break\n","        #   continue\n","\n","        \n","        temp_tp_list = []\n","        remain_tp = 2\n","        hedge_cnt = 1\n","        h_ep, h_tp = None, None\n","        h_i, h_j = None, None\n","        trade_done = False\n","        \n","        for j in range(i + 1, len(res_df)):\n","\n","          \n","          # -------------- hedge only once -------------- #\n","          #             ì¼ë‹¨, h_quantity ëŠ” ì´ˆê¸° ì§„ìž…ê³¼ ë™ì¼í•˜ê²Œ ì„¤ì •         #\n","          # if res_df['high'].iloc[j] >= res_df['minor_ST2_Up'].iloc[j] and hedge_cnt == 1:\n","          # if res_df['close'].iloc[j] >= res_df['minor_ST2_Up'].iloc[j] and hedge_cnt == 1:\n","          # if res_df['close'].iloc[j] >= res_df['minor_ST3_Up'].iloc[j] and hedge_cnt == 1:\n","\n","          #   h_ep = res_df['close'].iloc[j]\n","          #   hedge_cnt -= 1\n","          #   h_i = j\n","\n","\n","          # -------------- sub tp -------------- #\n","          # if res_df['low'].iloc[j] <= short_tp2.iloc[j] and remain_tp == 2:\n","          # # if res_df['low'].iloc[j] <= short_tp2.iloc[j] <= res_df['high'].iloc[j]: --> ì´ê±´ ìž˜ëª»ë˜ì—ˆìŒ\n","\n","          #   # trade_done = True # <-- sub tp ì—ëŠ” trade_done ì„ ì£¼ì§€ ì•ŠìŒ !\n","          #   remain_tp -= 1\n","\n","          #   #         dynamic tp        #\n","          #   if short_tp2.iloc[j] != short_tp2.iloc[j - 1]:\n","\n","          #     #   tp limit ì´ ë¶ˆê°€í•œ ê²½ìš° = open ì´ ì´ë¯¸, tp ë¥¼ ë„˜ì€ ê²½ìš° # \n","          #     if res_df['open'].iloc[j] < short_tp2.iloc[j]:\n","\n","          #       # tp = short_tp2.iloc[j]\n","          #       tp = res_df['open'].iloc[j]\n","          #       # tp_state_list.append(\"d-open\")\n","\n","          #     #   tp limit ì´ ê°€ëŠ¥í•œ ê²½ìš° = open ì´ ì•„ì§, tp ë¥¼ ë„˜ì§€ ì•Šì€ ê²½ìš° # \n","          #     else:\n","\n","          #       tp = short_tp2.iloc[j]\n","          #       # tp = res_df['open'].iloc[j]\n","          #       # tp_state_list.append(\"d-short_tp2\")\n","\n","          #   #         static tp         #\n","          #   else:\n","              \n","          #     #   tp limit ì´ ë¶ˆê°€í•œ ê²½ìš° = open ì´ ì´ë¯¸, tp ë¥¼ ë„˜ì€ ê²½ìš° # \n","          #     #   non_inversion ì˜ ê²½ìš°, short_tp2 ê°€ ê°€ëŠ¥í•¨   #\n","\n","          #     if res_df['open'].iloc[j] < short_tp2.iloc[j]:\n","\n","          #       tp = short_tp2.iloc[j]\n","          #       # tp = res_df['open'].iloc[j]\n","          #       # tp_state_list.append(\"s-short_tp2\")\n","\n","          #     #   tp limit ì´ ê°€ëŠ¥í•œ ê²½ìš° = open ì´ ì•„ì§, tp ë¥¼ ë„˜ì§€ ì•Šì€ ê²½ìš° # \n","          #     else:\n","\n","          #       tp = short_tp2.iloc[j]\n","          #       # tp = res_df['open'].iloc[j]\n","          #       # tp_state_list.append(\"s-short_tp2\")   \n","\n","          #   temp_tp_list.append(tp)  \n","\n","\n","          # -------------- ultimate tp -------------- #\n","          #               1. by price line             #\n","          if res_df['low'].iloc[j] <= short_tp.iloc[initial_i]: # we use static tp now\n","          # if res_df['low'].iloc[j] <= short_tp.iloc[j]:\n","          # if res_df['low'].iloc[j] <= short_tp.iloc[j] <= res_df['high'].iloc[j]: --> ì´ê±´ ìž˜ëª»ë˜ì—ˆìŒ\n","\n","            trade_done = True\n","\n","            #         dynamic tp        #\n","            if 0:\n","            # if short_tp.iloc[j] != short_tp.iloc[j - 1]:\n","\n","              #   tp limit ì´ ë¶ˆê°€í•œ ê²½ìš° = open ì´ ì´ë¯¸, tp ë¥¼ ë„˜ì€ ê²½ìš° # \n","              # if res_df['open'].iloc[j] < short_tp.iloc[initial_i]:\n","              if res_df['open'].iloc[j] < short_tp.iloc[j]:\n","\n","                # tp = short_tp.iloc[initial_i]\n","                tp = res_df['open'].iloc[j]\n","                tp_state_list.append(\"d-short_open\")\n","\n","              #   tp limit ì´ ê°€ëŠ¥í•œ ê²½ìš° = open ì´ ì•„ì§, tp ë¥¼ ë„˜ì§€ ì•Šì€ ê²½ìš° # \n","              else:\n","\n","                # tp = short_tp.iloc[init